.hot
	background #c0cfdb url(../image/bg2.png) no-repeat center center
	height 634px
	width 100%
	overflow hidden
	>h1
		display block
		text-align center
		font-size 28px
		color #fff
		margin-top 28px
	.hot-title
		margin 0 auto 22px
		text-align center
		>span
			display inline-block
			text-align center
			font-size 14px
			color #fff
			margin-top 28px
		>a
			display inline-block
			font-size 12px
			height 28px
			padding 0 10px
			margin-left 12px
			color white
			border 1px solid #fff
			border-radius 3px
			text-align center
			line-height @height
			cursor pointer
			&:hover
				border-color transparent
				background-color $green
	.content
		height 450px
		position relative
		i
			font-size 40px
			font-weight bold
			color rgb(165,165,165)
			margin-top 211px
			cursor pointer
			&:hover
				color $green
		i.left
			position absolute
			top 0
			left -60px
			display none
		i.right
			position absolute
			top 0
			right -40px
		.hidden
			float left
			width 1190px
			height 450px
			overflow hidden
			.hot-con
