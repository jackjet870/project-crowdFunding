@import "reset"
$green=rgb(0,204,206)
.main
	width 1210px
	margin 0 auto
.nav
	position relative
	z-index 10
	height 36px
	border-bottom 2px solid $green
	.main
		position relative
		margin-right auto
		margin-left @margin-right
		width 1190px
		.left
			float left
		.right
			float right
		.anchor
			float left
			position relative
			padding-right 40px
			padding-left @padding-right
			font-size 14px
			line-height 32px
			border-bottom-width 2px
			border-bottom-style solid
			border-bottom-color transparent
			cursor pointer
			&.current
				border-bottom-color $green
			&:hover
				color $green
				>a
					&:after
						background-image url("data:image/png;base64,")
						transform rotate(180deg)
				.fixedBox
					.items
						transform translateY(0)
			>a
				&:after
					margin-left 3px
					display inline-block
					width 9px
					height 6px
					background-image url("data:image/png;base64,")
					content ""
					transition transform .25s
					vertical-align middle
